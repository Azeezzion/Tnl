{
    "extensions": [
		"golang.go"
	],
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "1.18",
			"NODE_VERSION": "none"
		}
	},
	"settings": {
		"go.toolsManagement.checkForUpdates": "local",
		"go.useLanguageServer": true,
		"go.gopath": "/go"
	},
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"remoteUser": "vscode"
}